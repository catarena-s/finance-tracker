  cov_2kklejiyf7().f[19]++;
  cov_2kklejiyf7().s[30]++;
  return "h-64 sm:h-80 lg:h-96";
}
function getTouchTargetClasses() {
  /* istanbul ignore next */
  cov_2kklejiyf7().f[20]++;
  cov_2kklejiyf7().s[31]++;
