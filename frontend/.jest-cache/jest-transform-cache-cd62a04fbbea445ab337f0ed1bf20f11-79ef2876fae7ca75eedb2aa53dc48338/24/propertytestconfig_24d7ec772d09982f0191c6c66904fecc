/**
 * Конфигурация для property-based тестов
 * Позволяет настраивать количество прогонов в зависимости от окружения
 */ // Определяем количество прогонов на основе окружения
"use strict";
Object.defineProperty(exports, "__esModule", {
    value: true
});
function _export(target, all) {
    for(var name in all)Object.defineProperty(target, name, {
        enumerable: true,
        get: all[name]
    });
}
_export(exports, {
    NUM_RUNS: function() {
        return NUM_RUNS;
    },
    TEST_TIMEOUT: function() {
        return TEST_TIMEOUT;
    },
    getNumRuns: function() {
        return getNumRuns;
    },
    getTimeout: function() {
        return getTimeout;
    }
});
const isCI = process.env.CI === "true" || process.env.NODE_ENV === "test";
const isFastMode = process.env.FAST_TESTS === "true";
const NUM_RUNS = {
    // Быстрые тесты (простые проверки)
    FAST: isCI ? 3 : isFastMode ? 10 : 50,
    // Средние тесты (стандартные проверки)
    MEDIUM: isCI ? 5 : isFastMode ? 20 : 100,
    // Медленные тесты (сложные проверки с рендерингом)
    SLOW: isCI ? 2 : isFastMode ? 5 : 20,
    // Очень медленные тесты (интеграционные)
    VERY_SLOW: isCI ? 1 : isFastMode ? 3 : 10
};
const TEST_TIMEOUT = {
    FAST: 10000,
    MEDIUM: 15000,
    SLOW: 30000,
    VERY_SLOW: 45000
};
const getNumRuns = (type = "MEDIUM")=>{
    return NUM_RUNS[type];
};
const getTimeout = (type = "MEDIUM")=>{
    return TEST_TIMEOUT[type];
};

