        (cov_1njoiput7x().b[8][0]++, "Обновить") :
        /* istanbul ignore next */
        (cov_1njoiput7x().b[8][1]++, "Создать")
      }), /*#__PURE__*/(0, _jsxruntime.jsx)(_ui.Button, {
        type: "button",
        variant: "secondary",
        onClick: onCancel,
        disabled: isSubmitting,
        className: "flex-1",
        children: "Отмена"
      })]
    })]
  });
}
