    }, 0) / bucket.length);
    // Используем дату первой точки в группе
    /* istanbul ignore next */
    cov_2acajwui3g().s[31]++;
    result.push({
      date: bucket[0].date,
      amount: Math.round(avgAmount * 100) / 100
    });
  }
  /* istanbul ignore next */
  cov_2acajwui3g().s[32]++;
  return result;
}
