function optimizeGenericData(data, isMobile, isTablet) {
  /* istanbul ignore next */
  cov_2acajwui3g().f[10]++;
  cov_2acajwui3g().s[39]++;
  // Если данных меньше порога - не оптимизируем
  if (
  /* istanbul ignore next */
  (cov_2acajwui3g().b[12][0]++, !data) ||
  /* istanbul ignore next */
  (cov_2acajwui3g().b[12][1]++, data.length <= OPTIMIZATION_THRESHOLD)) {
    /* istanbul ignore next */
    cov_2acajwui3g().b[11][0]++;
    cov_2acajwui3g().s[40]++;
    return data;
  } else
  /* istanbul ignore next */
  {
    cov_2acajwui3g().b[11][1]++;
  }
  // Определяем максимальное количество точек для устройства
  const maxPoints =
  /* istanbul ignore next */
  (cov_2acajwui3g().s[41]++, isMobile ?
  /* istanbul ignore next */
  (cov_2acajwui3g().b[13][0]++, MAX_DATA_POINTS.mobile) :
  /* istanbul ignore next */
  (cov_2acajwui3g().b[13][1]++, isTablet ?
  /* istanbul ignore next */
  (cov_2acajwui3g().b[14][0]++, MAX_DATA_POINTS.tablet) :
  /* istanbul ignore next */
  (cov_2acajwui3g().b[14][1]++, MAX_DATA_POINTS.desktop)));
  // Если данных меньше максимума для устройства - не оптимизируем
  /* istanbul ignore next */
  cov_2acajwui3g().s[42]++;
  if (data.length <= maxPoints) {
    /* istanbul ignore next */
    cov_2acajwui3g().b[15][0]++;
    cov_2acajwui3g().s[43]++;
    return data;
  } else
  /* istanbul ignore next */
  {
    cov_2acajwui3g().b[15][1]++;
  }
  // Применяем sampling
  const result =
  /* istanbul ignore next */
  (cov_2acajwui3g().s[44]++, []);
  const step =
  /* istanbul ignore next */
  (cov_2acajwui3g().s[45]++, (data.length - 1) / (maxPoints - 1));
  // Всегда включаем первый элемент
  /* istanbul ignore next */
  cov_2acajwui3g().s[46]++;
  result.push(data[0]);
  // Выбираем элементы через равные интервалы
  /* istanbul ignore next */
  cov_2acajwui3g().s[47]++;
  for (let i =
  /* istanbul ignore next */
  (cov_2acajwui3g().s[48]++, 1); i < maxPoints - 1; i++) {
    const index =
    /* istanbul ignore next */
    (cov_2acajwui3g().s[49]++, Math.round(i * step));
    /* istanbul ignore next */
    cov_2acajwui3g().s[50]++;
    result.push(data[index]);
  }
  // Всегда включаем последний элемент
  /* istanbul ignore next */
  cov_2acajwui3g().s[51]++;
  result.push(data[data.length - 1]);
  /* istanbul ignore next */
  cov_2acajwui3g().s[52]++;
  return result;
}
function shouldOptimizeData(dataLength, isMobile, isTablet) {
  /* istanbul ignore next */
  cov_2acajwui3g().f[11]++;
  cov_2acajwui3g().s[53]++;
  if (dataLength <= OPTIMIZATION_THRESHOLD) {
    /* istanbul ignore next */
    cov_2acajwui3g().b[16][0]++;
    cov_2acajwui3g().s[54]++;
    return false;
  } else
  /* istanbul ignore next */
  {
    cov_2acajwui3g().b[16][1]++;
  }
  const maxPoints =
  /* istanbul ignore next */
  (cov_2acajwui3g().s[55]++, isMobile ?
  /* istanbul ignore next */
  (cov_2acajwui3g().b[17][0]++, MAX_DATA_POINTS.mobile) :
  /* istanbul ignore next */
  (cov_2acajwui3g().b[17][1]++, isTablet ?
  /* istanbul ignore next */
  (cov_2acajwui3g().b[18][0]++, MAX_DATA_POINTS.tablet) :
  /* istanbul ignore next */
  (cov_2acajwui3g().b[18][1]++, MAX_DATA_POINTS.desktop)));
  /* istanbul ignore next */
  cov_2acajwui3g().s[56]++;
  return dataLength > maxPoints;
}
