        month: "long",
        day: "numeric"
      }).format(dateObj);
    case "short":
      /* istanbul ignore next */
      cov_21cj8vvz5l().b[4][2]++;
    default:
      /* istanbul ignore next */
      cov_21cj8vvz5l().b[4][3]++;
      cov_21cj8vvz5l().s[14]++;
      return new Intl.DateTimeFormat("en-US", {
        year: "numeric",
        month: "2-digit",
        day: "2-digit"
      }).format(dateObj);
  }
}
function parseDate(dateString) {
  /* istanbul ignore next */
  cov_21cj8vvz5l().f[6]++;
  cov_21cj8vvz5l().s[15]++;
  // Если строка уже содержит информацию о часовом поясе, используем её
  if (
  /* istanbul ignore next */
  (cov_21cj8vvz5l().b[6][0]++, dateString.includes("T")) ||
  /* istanbul ignore next */
  (cov_21cj8vvz5l().b[6][1]++, dateString.includes("Z"))) {
    /* istanbul ignore next */
    cov_21cj8vvz5l().b[5][0]++;
    cov_21cj8vvz5l().s[16]++;
    return new Date(dateString);
  } else
  /* istanbul ignore next */
  {
    cov_21cj8vvz5l().b[5][1]++;
  }
  // Для дат без времени (YYYY-MM-DD), создаем дату в локальном часовом поясе
  const [year, month, day] =
  /* istanbul ignore next */
  (cov_21cj8vvz5l().s[17]++, dateString.split("-").map(Number));
  /* istanbul ignore next */
  cov_21cj8vvz5l().s[18]++;
  return new Date(year, month - 1, day);
}
