const _responsiveConfig = require("../../src/lib/responsiveConfig");
describe("ResponsiveConfig", ()=>{
    describe("breakpoints", ()=>{
        it("should define all standard Tailwind breakpoints", ()=>{
            expect(_responsiveConfig.responsiveConfig.breakpoints).toEqual({
                sm: 640,
                md: 768,
                lg: 1024,
                xl: 1280,
                "2xl": 1440
            });
        });
        it("should have breakpoints in ascending order", ()=>{
            const { sm, md, lg, xl } = _responsiveConfig.responsiveConfig.breakpoints;
            expect(sm).toBeLessThan(md);
            expect(md).toBeLessThan(lg);
            expect(lg).toBeLessThan(xl);
            expect(xl).toBeLessThan(_responsiveConfig.responsiveConfig.breakpoints["2xl"]);
        });
    });
    describe("touchTargetSize", ()=>{
        it("should meet WCAG AAA minimum touch target size of 44px", ()=>{
            // Requirement 2.4: минимальная область касания 44x44px
            expect(_responsiveConfig.responsiveConfig.touchTargetSize.minimum).toBe(44);
        });
        it("should define comfortable touch target size", ()=>{
            expect(_responsiveConfig.responsiveConfig.touchTargetSize.comfortable).toBe(48);
        });
        it("should have comfortable size greater than minimum", ()=>{
            expect(_responsiveConfig.responsiveConfig.touchTargetSize.comfortable).toBeGreaterThan(_responsiveConfig.responsiveConfig.touchTargetSize.minimum);
        });
    });
    describe("spacing", ()=>{
        it("should define mobile spacing with 16px (px-4)", ()=>{
            // Requirement 1.4: минимальные отступы 16px по краям на мобильных
            expect(_responsiveConfig.responsiveConfig.spacing.mobile.container).toBe("px-4");
            expect(_responsiveConfig.responsiveConfig.spacing.mobile.card).toBe("p-4");
            expect(_responsiveConfig.responsiveConfig.spacing.mobile.gap).toBe("gap-4");
        });
        it("should define tablet spacing with 24px (px-6)", ()=>{
            expect(_responsiveConfig.responsiveConfig.spacing.tablet.container).toBe("sm:px-6");
            expect(_responsiveConfig.responsiveConfig.spacing.tablet.card).toBe("sm:p-6");
            expect(_responsiveConfig.responsiveConfig.spacing.tablet.gap).toBe("sm:gap-6");
        });
        it("should define desktop spacing with 32px (px-8)", ()=>{
            expect(_responsiveConfig.responsiveConfig.spacing.desktop.container).toBe("lg:px-8");
            expect(_responsiveConfig.responsiveConfig.spacing.desktop.card).toBe("lg:p-8");
            expect(_responsiveConfig.responsiveConfig.spacing.desktop.gap).toBe("lg:gap-8");
        });
        it("should have consistent structure across all device sizes", ()=>{
            const { mobile, tablet, desktop } = _responsiveConfig.responsiveConfig.spacing;
            expect(mobile).toHaveProperty("container");
            expect(mobile).toHaveProperty("card");
            expect(mobile).toHaveProperty("gap");
